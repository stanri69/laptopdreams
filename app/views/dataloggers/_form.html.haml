= form_for @datalogger do |f|
  - if @datalogger.errors.any?
    #error_explanation
      %h2= "#{pluralize(@datalogger.errors.count, "error")} prohibited this datalogger from being saved:"
      %ul
        - @datalogger.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :volume
    = f.number_field :volume
  .field
    = f.label :type_datalogger
    = f.text_field :type_datalogger
  .actions
    = f.submit 'Save'
